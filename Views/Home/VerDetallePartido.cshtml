
<p>HOLA</p>
<h2>@ViewBag.Partido.Nombre</h2>
<img src="@ViewBag.Partido.Logo" alt="Foto Partido">
<p>@ViewBag.Partido.FechaFundacion</p>
<p>Cantidad diputados: </p>
<p>@ViewBag.Partido.CantidadDiputados</p>
<p>Cantidad senadores: </p>
<p>@ViewBag.Partido.CantidadSenadores</p>

@foreach (var item in ViewBag.CandidatosPartido)
{
  <section class="candidato-particular">
    <a href='@Url.Action("VerDetalleCandidato", new {IdCandidato = item.IdCandidato})'>
    <h1>@item.Nombre @item.Apellido</h1>
    </a>
    <a href='@Url.Action("EliminarCandidato",new {IdCandidato = item.IdCandidato,IdPartido=item.IdPartido})'><h3>Eliminar Candidato</h3></a>
  </section>
}

<a href='@Url.Action("AgregarCandidato", new{IdPartido = ViewBag.Partido.IdPartido})'><p>Agregar un candidato.</p></a>

